{
  "name": "system-server",
  "version": "0.8.0-alpha.0",
  "description": "laf - system server",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "private": "true",
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "npx tsc -p tsconfig.json",
    "watch": "npx tsc -p tsconfig.json -w",
    "init": "node init/index.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "BaaS",
    "cloud",
    "laf.js",
    "serverless",
    "laf",
    "devops"
  ],
  "dependencies": {
    "@aws-sdk/client-s3": "^3.67.0",
    "@aws-sdk/client-sts": "^3.67.0",
    "@types/adm-zip": "^0.4.34",
    "adm-zip": "^0.5.9",
    "axios": "^0.24.0",
    "body-parser": "^1.19.0",
    "database-proxy": "^0.7.10",
    "dayjs": "^1.10.6",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.3.0",
    "minio": "^7.0.27",
    "mongodb": "^4.1.3",
    "mongodb-uri": "^0.9.7",
    "multer": "^1.4.4",
    "nanoid": "^3.3.3"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mongodb-uri": "^0.9.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.7.10",
    "nodemon": "^2.0.12",
    "typescript": "^4.2.3"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "src/*",
      "http/*"
